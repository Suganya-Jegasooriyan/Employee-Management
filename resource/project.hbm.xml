<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name = "com.ideas2it.employeemanagement.model.EmployeeProject" table = "PROJECT">
      
      <id name = "projectId" column = "PROJECT_ID">
         <generator class="native"/>
      </id>
      
      <property name = "customerId" column = "CUSTOMER_ID"/>
      <property name = "projectName" column = "PROJECT_NAME"/>
      <property name = "customerMailId" column = "CUSTOMER_MAIL_ID"/>
      <property name = "dateOfStart" column = "DATE_OF_START"/>
      <property name = "dateOfEnd" column = "DATE_OF_END"/>

      <list name="employeeDetails" table="employee_project" fetch="select" cascade="all">  
            <key column="PROJECT_ID" />  
               <index column="type"></index>   
            <many-to-many class="com.ideas2it.employeemanagement.model.Employee" column="ID" />  
        </list>

   </class>
</hibernate-mapping>