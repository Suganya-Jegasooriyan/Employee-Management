<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name = "com.ideas2it.employeemanagement.model.Address" table = "ADDRESSTABLE">
      
      <id name = "addressId" column = "ADDRESS_ID">
         <generator class = "native"/>
      </id>
      
      <property name = "firstLine" column = "FIRST_LINE"/>
      <property name = "secondLine" column = "SECOND_LINE"/>
      <property name = "city" column = "CITY"/>
      <property name = "district" column = "DISTRICT"/>
      <property name = "state" column = "STATE"/>
      <property name = "pinCode" column = "PINCODE"/>
      <property name = "addressType" column = "ADDRESS_TYPE"/>
      
   </class>
</hibernate-mapping>